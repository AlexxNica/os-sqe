{
  "NovaServers.boot_server_from_volume": [
    {
      "args": {
        "flavor": {
          "name": "m1.small"
        },
        "image": {
          "name": "^Ubuntu*"
        },
        "volume_size": 3
      },
      "context": {
        "quotas": {
          "cinder": {
            "gigabytes": -1,
            "snapshots": -1,
            "volumes": -1
          },
          "neutron": {
            "floatingip": -1,
            "network": -1,
            "port": -1,
            "router": -1,
            "security_group": -1,
            "security_group_rule": -1,
            "subnet": -1
          },
          "nova": {
            "cores": -1,
            "instances": -1,
            "metadata_items": -1,
            "ram": -1
          }
        },
        "users": {
          "project_domain": "default",
          "resource_management_workers": 30,
          "static_tenant_match": "ctx_rally",
          "static_username_match": "ctx_rally",
          "tenants": 45,
          "use_static_tenants": false,
          "use_static_users": false,
          "user_domain": "default",
          "user_password": "password",
          "users_per_tenant": 3
        }
      },
      "runner": {
        "concurrency": 5,
        "times": 500,
        "type": "constant"
      }
    }
  ]
}
