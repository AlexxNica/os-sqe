          </logTexts>
          <EscalateStatus>false</EscalateStatus>
          <RunIfJobSuccessful>false</RunIfJobSuccessful>
          <script>ssh localadmin@172.29.173.228 &quot;mkdir -p /opt/www/${JOB_NAME}/${BUILD_NUMBER}&quot; || :&#xd;
scp localadmin@172.29.173.133:/var/lib/jenkins/jobs/$JOB_NAME/builds/$BUILD_NUMBER/log $WORKSPACE/openstack-sqe/build.log || :&#xd;
scp localadmin@172.29.173.133:/var/lib/jenkins/jobs/$JOB_NAME/builds/$BUILD_NUMBER/changelog.xml $WORKSPACE/openstack-sqe/changelog.xml || :&#xd;
gzip $WORKSPACE/openstack-sqe/build.log || :&#xd;
&#xd;
scp -r test_data/* localadmin@172.29.173.228:/opt/www/${JOB_NAME}/${BUILD_NUMBER}/&#xd;
&#xd;
scp openstack-sqe/config_file result.xml openstack-sqe/build* openstack-sqe/*.xml openstack-sqe/openrc openstack-sqe/testr_results.subunit tempest/etc/tempest.conf localadmin@172.29.173.228:/opt/www/${JOB_NAME}/${BUILD_NUMBER}/&#xd;
&#xd;
scp result.xml localadmin@172.29.173.229:~/workspace/2role_regular/&#xd;
scp nosetests_2role.xml localadmin@172.29.173.229:~/workspace/COI_nightly/&#xd;
scp nosetests_2role.xml localadmin@172.29.173.229:~/workspace/AIO_2role_tempest_compare/</script>
        </hudson.plugins.postbuildtask.TaskProperties>
      </tasks>
    </hudson.plugins.postbuildtask.PostbuildTask>
