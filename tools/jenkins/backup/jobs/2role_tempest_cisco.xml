  <builders>
    <hudson.tasks.Shell>
      <command>cd $WORKSPACE/openstack-sqe/
fab init
fab coi.full:2role
#make full-2role
if grep -q tempest $WORKSPACE/openstack-sqe/tools/tempest-scripts/tests_set; then
fab coi.full:2role
#make full-2role
fi
python $WORKSPACE/openstack-sqe/tools/tempest-scripts/junit_merge.py $(ls $WORKSPACE/openstack-sqe/nosetests*xml) &gt; $WORKSPACE/result.xml
cp $WORKSPACE/result.xml ${WORKSPACE}/nosetests_cisco.xml
cp $WORKSPACE/result.xml ${WORKSPACE}/nosetests_2role.xml</command>
    </hudson.tasks.Shell>
  </builders>
